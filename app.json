{
  "expo": {
    "name": "Kord Snap & Solve",
    "slug": "kord-snap-and-solve",
    "version": "1.0.1",
    "owner": "kord_snap_and_solve",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "scheme": "kord-snap-and-solve",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./src/assets/images/big_logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kordsnapandsolve.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/logo.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.kordsnapandsolve.app",
      "versionCode": 1,
      "permissions": [
        "CAMERA"
      ]
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "react-native-document-scanner-plugin",
        {
          "cameraPermission": "$(PRODUCT_NAME) needs camera access to scan questions"
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "usesCleartextTraffic": true
          }
        }
      ],
      "expo-font"
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1732b941-53cb-4dc1-a17e-ebe13425ff72"
      }
    }
  }
}
